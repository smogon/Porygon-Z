{
  "name": "porygon-z",
  "version": "0.0.1",
  "description": "A discord bot for the official Smogon discord https://discord.gg/smogon",
  "main": "./dist/app.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint . --cache --ext .ts && tsc",
    "fix": "eslint . --cache --ext .ts --fix",
    "full-lint": "eslint . --cache --ext .ts --max-warnings 0",
    "start": "node build && node -r dotenv/config bot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smogon/Porygon-Z.git"
  },
  "author": "Andrew Werner",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.7.8",
    "@types/pg": "^7.14.1",
    "@types/ws": "^7.2.3",
    "discord.js": "^12.1.1",
    "dotenv": "^8.1.0",
    "eslint-plugin-jest": "^24.3.5",
    "pg": "^8.0.3",
    "pg-mem": "^1.9.6",
    "postgrator": "^4.0.1",
    "sqlutils": "^1.2.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@pkmn/eslint-config": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "tslint": "^5.20.1"
  }
}
